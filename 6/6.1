#include <iostream>
#include <iomanip> // setw()
using namespace std;

int main() {
    const int SIZE = 9;
    int table[SIZE][SIZE];

    // заполнение массива таблицей умножения
    for (int i = 0; i < SIZE; ++i) {
        for (int j = 0; j < SIZE; ++j) {
            table[i][j] = (i + 1) * (j + 1);
        }
    }

    // вывод таблицы умножения
    for (int i = 0; i < SIZE; ++i) {
        for (int j = 0; j < SIZE; ++j) {
            cout << setw(4) << table[i][j];
        }
        cout << endl;
    }

    return 0;
}
